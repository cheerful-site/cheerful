<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.korit.cheerful_back.domain.admin.AdminMapper">

  <select id="findByAdminId" resultType="com.korit.cheerful_back.domain.admin.Admin">
    select
      *
    from
      admin_tb
    where
      admin_login_id = #{adminLoginId}
  </select>


</mapper>