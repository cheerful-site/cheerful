<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.korit.cheerful_back.domain.communityImg.CommunityImgMapper">

    <insert id="insert">
        insert into community_img_tb
        values (default, #{communityId}, #{seq}, #{imgPath})
    </insert>

    <insert id="insertMany">
        insert into community_img_tb
        values
        <foreach item="communityImg" collection="communityImgs" separator=",">
            (default, #{communityImg.communityId}, #{communityImg.seq}, #{communityImg.imgPath})
        </foreach>
    </insert>
</mapper>