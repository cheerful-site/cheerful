<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.korit.cheerful_back.domain.foodCommentImg.FoodCommentImgMapper">
  <insert id="insertMany">
    insert into food_comment_img_tb
    values
    <foreach item="foodCommentImg" collection= "foodCommentImgs" separator=",">
      (default, #{foodCommentImg.foodCommentId}, #{foodCommentImg.seq}, #{foodCommentImg.imgPath})
    </foreach>
  </insert>
</mapper>