<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.korit.cheerful_back.domain.foodImg.FoodImgMapper">
  <insert id="insert">
    insert into food_img_tb
    values (default, #{foodId}, #{seq}, #{imgPath})
  </insert>

  <insert id="insertMany">
    insert into food_img_tb
    values
    <foreach item="foodImg" collection= "foodImgs" separator=",">
      (default, #{foodImg.foodId}, #{foodImg.seq}, #{foodImg.imgPath})
    </foreach>
  </insert>
</mapper>