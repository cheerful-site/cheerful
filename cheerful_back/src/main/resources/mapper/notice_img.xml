<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.korit.cheerful_back.domain.noticeImg.NoticeImgMapper">

    <insert id="insert">
        insert into notice_img_tb
        values (default, #{noticeId}, #{seq}, #{imgPath});
    </insert>

    <insert id="insertMany">
        insert into notice_img_tb
        values
        <foreach item="noticeImg" collection="noticeImgs" separator=",">
            (default, #{noticeImg.noticeId}, #{noticeImg.seq}, #{noticeImg.imgPath})
        </foreach>
    </insert>

</mapper>